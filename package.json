{
  "name": "fix-frontend",
  "description": "Fix is an open-source Wiz alternative for cloud infrastructure security. Take control of cloud risks with an asset inventory, compliance scans, and remediation workflows.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "lint:tsc:org": "tsc --noEmit",
    "lint:tsc": "tsc-files --noEmit",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "postinstall": "patch-package && husky install",
    "prebuild": "yarn i18n:compile",
    "prepare": "husky install",
    "prestart": "yarn i18n:compile",
    "pretest": "yarn i18n:compile",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "preview:storybook": "vite preview --outDir=\"storybook-static\"",
    "start": "vite",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/nunito-sans": "^5.0.9",
    "@lingui/react": "^4.6.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/lab": "^5.0.0-alpha.158",
    "@mui/material": "^5.15.2",
    "@mui/system": "^5.15.2",
    "@mui/x-date-pickers": "^6.18.6",
    "@mui/x-tree-view": "^6.17.0",
    "@tanstack/react-query": "^5.15.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.6.3",
    "d3": "^7.8.5",
    "dayjs": "^1.11.10",
    "flag-icons": "^7.1.0",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "highlight.js": "^11.9.0",
    "lowlight": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-router-dom": "^6.21.1",
    "react-transition-group": "^4.4.5",
    "react-window": "^1.8.10",
    "recharts": "^2.10.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@lingui/cli": "^4.6.0",
    "@lingui/macro": "^4.6.0",
    "@lingui/react": "^4.6.0",
    "@lingui/swc-plugin": "^4.0.4",
    "@lingui/vite-plugin": "^4.6.0",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/blocks": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/react-vite": "7.6.6",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^1.1.0",
    "babel-plugin-macros": "3.1.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "mockjs": "^1.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prop-types": "15",
    "react-refresh": "^0.14.0",
    "storybook": "7.6.6",
    "tsc-files": "^1.1.4",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "unique-names-generator": "^4.7.1",
    "vite": "^5.0.10",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.1.0",
    "web-vitals": "^3.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn format:check"
    ]
  },
  "engines": {
    "node": "^18.17.1 || ^20.9.0",
    "yarn": "^1.22.19"
  }
}
