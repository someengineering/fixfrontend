{
  "name": "fix-frontend",
  "description": "Fix is an open-source Wiz alternative for cloud infrastructure security. Take control of cloud risks with an asset inventory, compliance scans, and remediation workflows.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "lint:tsc:org": "tsc --noEmit",
    "lint:tsc": "tsc-files --noEmit",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "postinstall": "shx cp ./node_modules/trackjs/t.js ./public/t.min.js",
    "prebuild": "yarn i18n:compile",
    "prepare": "husky",
    "prestart": "yarn i18n:compile",
    "pretest": "yarn i18n:compile",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "preview:storybook": "vite preview --outDir=\"storybook-static\"",
    "start": "vite",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --run --config=./vite.config.test.ts",
    "test:dev": "vitest --config=./vite.config.test.ts",
    "test:coverage:all": "yarn test --coverage",
    "test:coverage": "yarn test --coverage.enabled=true --coverage.all=false",
    "test:inspect": "yarn test --inspect --no-file-parallelism --maxConcurrency=1 --bail=1",
    "test:ui": "yarn test --ui"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.2",
    "@emotion/react": "^11.11.4",
    "@fontsource-variable/nunito-sans": "^5.0.14",
    "@lingui/macro": "^4.11.0",
    "@lingui/react": "^4.11.0",
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@mui/system": "^5.15.15",
    "@mui/x-charts": "^7.5.1",
    "@mui/x-data-grid-premium": "^7.5.1",
    "@mui/x-date-pickers-pro": "^7.5.1",
    "@mui/x-tree-view": "^7.5.1",
    "@tanstack/react-query": "^5.40.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.7.2",
    "d3": "^7.9.0",
    "dayjs": "^1.11.11",
    "diff": "^5.2.0",
    "flag-icons": "^7.2.3",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "highlight.js": "^11.9.0",
    "immer": "^10.1.1",
    "js-cookie": "^3.0.5",
    "lowlight": "^3.1.0",
    "mustache": "^4.2.0",
    "posthog-js": "^1.135.2",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.23.1",
    "react-transition-group": "^4.4.5",
    "react-window": "^1.8.10",
    "recharts": "^2.12.7",
    "trackjs": "^3.10.4",
    "typescript-parsec": "^0.3.4",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@emotion/styled": "^11.11.5",
    "@lingui/cli": "^4.11.0",
    "@lingui/swc-plugin": "^4.0.7",
    "@lingui/vite-plugin": "^4.11.0",
    "@storybook/addon-essentials": "^8.1.4",
    "@storybook/addon-links": "^8.1.4",
    "@storybook/blocks": "^8.1.4",
    "@storybook/react": "^8.1.4",
    "@storybook/react-vite": "^8.1.4",
    "@storybook/test": "^8.1.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@types/d3": "^7.4.3",
    "@types/diff": "^5.2.1",
    "@types/js-cookie": "^3.0.6",
    "@types/mustache": "^4.2.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react-swc": "~3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "babel-plugin-macros": "3.1.0",
    "esbuild": "^0.21.3",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prop-types": "15",
    "react-refresh": "^0.14.2",
    "shx": "^0.3.4",
    "storybook": "^8.1.4",
    "tsc-files": "^1.1.4",
    "type-fest": "^4.18.3",
    "typescript": "^5.4.5",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.1",
    "vite": "^5.2.12",
    "vite-plugin-mock-dev-server": "^1.5.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0",
    "web-vitals": "^4.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn lint:tsc:files"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn format:check"
    ]
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0",
    "yarn": "^1.22.19"
  }
}
