{
  "name": "fix-frontend",
  "description": "Fix is an open-source Wiz alternative for cloud infrastructure security. Take control of cloud risks with an asset inventory, compliance scans, and remediation workflows.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "lint:tsc": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --max-warnings 0 && yarn lint:tsc",
    "postinstall": "patch-package && husky install",
    "prebuild": "yarn i18n:compile",
    "prepare": "husky install",
    "prestart": "yarn i18n:compile",
    "pretest": "yarn i18n:compile",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "preview:storybook": "vite preview --outDir=\"storybook-static\"",
    "start": "vite",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/nunito-sans": "^5.0.9",
    "@lingui/react": "^4.5.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/lab": "^5.0.0-alpha.149",
    "@mui/material": "^5.14.14",
    "@mui/system": "^5.14.14",
    "@mui/x-tree-view": "^6.0.0-beta.0",
    "@tanstack/react-query": "^5.0.5",
    "axios": "^1.5.1",
    "d3": "^7.8.5",
    "flag-icons": "^6.11.1",
    "lorem-ipsum": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.17.0",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.9.0"
  },
  "devDependencies": {
    "@lingui/cli": "^4.5.0",
    "@lingui/macro": "^4.5.0",
    "@lingui/swc-plugin": "^4.0.4",
    "@lingui/vite-plugin": "^4.5.0",
    "@storybook/addon-essentials": "7.5.1",
    "@storybook/addon-interactions": "7.5.1",
    "@storybook/addon-links": "7.5.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.1",
    "@storybook/react": "7.5.1",
    "@storybook/react-vite": "7.5.1",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/ui": "^0.34.6",
    "babel-plugin-macros": "3.1.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "mockjs": "^1.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prop-types": "15",
    "react-refresh": "^0.14.0",
    "storybook": "7.5.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-svgr": "^4.1.0",
    "vitest": "^0.34.6",
    "web-vitals": "^3.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn i18n:extract"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "yarn format"
    ]
  },
  "engines": {
    "node": "^18.17.1",
    "yarn": "^1.22.19"
  }
}
